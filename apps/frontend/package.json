{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "28.1.8",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.26.0",
    "d3": "^7.6.1",
    "react-imask": "^6.4.2",
    "react-scripts": "5.0.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "temperaturechart": "^1.0.0",
    "web-push": "^3.6.6",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot  --port 3000 --config webpack.dev.js",
    "start:prod": "webpack-dev-server --open --mode development --hot  --port 3000 --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "serve:prod": "serve -s dist -l 3000",
    "startCRA": "react-scripts start",
    "buildCRA": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.test\\.(ts|tsx|js|jsx)$",
      "\\.spec\\.(ts|tsx|js|jsx)$"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov", 
      "clover",
      "json-summary"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(d3|d3-.*|internmap|delaunator|robust-predicates)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@observablehq/plot": "^0.5.2",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^12.0.1",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "28.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
