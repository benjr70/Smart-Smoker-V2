{
  "name": "smoker",
  "version": "0.1.0",
  "private": true,
  "description": "Smart Smoker",
  "author": "Ben Rolf",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.7",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@mui/icons-material": "^5.7.0",
    "@mui/material": "^5.7.0",
    "axios": "^0.27.2",
    "d3": "^7.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-simple-keyboard": "^3.5.72",
    "rxjs": "^7.5.5",
    "serialport": "^10.4.0",
    "socket.io-client": "^4.5.1",
    "temperaturechart": "^1.0.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "websocket": "^1.0.34"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot --config webpack.dev.js",
    "start:electron": "electron .",
    "start:prod": "webpack-dev-server --open --mode development --hot --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "serve:prod": "serve -s dist -l 8080",
    "startCRA": "react-scripts start",
    "buildCRA": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "forge:thin": "cross-env ELECTRON_APP_MODE=thin electron-forge make",
    "forgeLinux64:thin": "npm run forge:thin  -- --platform=linux --arch=armv7l",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "main": "./.webpack/main",
  "config": {
    "forge": "./config.forge.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(d3|d3-.*|internmap|delaunator|robust-predicates)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text",
      "html",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "6.0.5",
    "@electron-forge/maker-deb": "6.0.5",
    "@electron-forge/maker-rpm": "6.0.5",
    "@electron-forge/maker-squirrel": "6.0.5",
    "@electron-forge/maker-wix": "6.0.5",
    "@electron-forge/maker-zip": "6.0.5",
    "@electron-forge/plugin-webpack": "6.0.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "28.1.8",
    "@types/node": "^24.3.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/websocket": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "electron": "^24.6.0",
    "electron-builder": "23.3.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "28.0.3",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
